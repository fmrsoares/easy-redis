{
  "name": "easy-redis",
  "displayName": "easy-redis",
  "description": "To access redis easily",
  "version": "0.0.1",
  "publisher": "Youngki Kang",
  "engines": {
    "vscode": "^1.30.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onView:redisExplorer",
    "onCommand:config.commands.redisServer"
  ],
  "main": "./out/extension",
  "contributes": {
    "configuration": [
      {
        "title": "Easy Redis Configuration",
        "properties": {
          "easyRedis.address": {
            "type": "string",
            "default": "localhost",
            "description": "Redis Server Address: Where redis client connect to"
          }
        }
      }
    ],
    "views": {
      "explorer": [
        {
          "id": "redisExplorer",
          "name": "Redis Explorer"
        }
      ]
    },
    "commands": [
      {
        "command": "config.commands.redisServer",
        "title": "Redis Server"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^3.1.4",
    "vscode": "^1.1.21",
    "tslint": "^5.8.0",
    "@types/node": "^8.10.25",
    "@types/mocha": "^2.2.42"
  },
  "dependencies": {
    "redis": "^2.8.0"
  }
}
